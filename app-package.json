{
  "name": "egg-react-cli",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "egg-port": "7001",
  "port": "7000",
  "proxy": {
    "/": {
      "target": "http://localhost:7000",
      "secure": false
    }
  },
  "scripts": {
    "bin": "node config/cli/bin.js",
    "dll": "webpack --config config/cli/webpack.vendor.dll.js",
    "dll-all": "cross-env dll=all npm run dll",
    "dll-react": "cross-env dll=react npm run dll",
    "dll-vue": "cross-env dll=vue npm run dll",
    "start": "webpack-dev-server --config config/cli/webpack.dev.js --open",
    "watch": "webpack --config config/cli/webpack.dev.js --watch ",
    "build": "webpack --progress --config config/cli/webpack.prod.js",
    "reload": "npm run build && npm run stop && npm run prod",
    "prod": "egg-scripts start --sticky --daemon --title=egg-server-testwb",
    "stop": "egg-scripts stop --title=egg-server-testwb",
    "dev": "egg-bin dev --sticky",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "dependencies": {
    "animejs": "^2.2.0",
    "antd": "^3.2.0",
    "lodash": "^4.17.5",
    "babel-preset-react": "^6.24.1",
    "mobx": "^3.5.1",
    "mobx-react": "^4.4.1",
    "normalize.css": "^7.0.0",
    "polyfill-exp": "^0.0.11",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-motion": "^0.5.2",
    "react-router-dom": "^4.2.2",
    "socket.io-client": "^2.0.4",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.101",
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "autoprefixer": "^7.2.5",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.18",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "csv-loader": "^2.1.1",
    "egg": "^2.2.1",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-imports": "^0.0.8",
    "egg-mock": "^3.14.0",
    "egg-mongodb": "^1.0.7",
    "egg-scripts": "^2.5.0",
    "egg-socket.io": "^4.0.6",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^6.0.0",
    "file-loader": "^1.1.6",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jsxobj": "^1.1.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.0",
    "react-hot-loader": "^4.0.0-beta.21",
    "style-loader": "^0.20.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "^0.6.2",
    "vue-loader": "^14.1.1",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1",
    "webstorm-disable-index": "^1.2.0",
    "xml-loader": "^1.2.1"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "bin": {
    "egg-react-cli": "config/cli/bin.js"
  },
  "license": "MIT"
}
